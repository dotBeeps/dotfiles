{
  "name": "CapyLock",
  "author": "Capybara",
  "version": "v0.1",
  "manifest_version": 9,
  "tabs": {
    "default": [
      "bigpicture"
    ]
  },
  "inject": {
    "cl_default.css": [],
    "cl_header.css": []
  },
  "patches": {
    "Lockscreen Background": {
      "type": "dropdown",
      "default": "Solid Color",
      "values": {
        "Solid Color": {},
        "Gradient": {
          "cl_lockbg_gradient.css": []
        },
        "Image": {
          "cl_lockbg_image.css": []
        }
      },
      "components": [
        {
          "name": "Color Picker",
          "type": "color-picker",
          "on": "Solid Color",
          "default": "#1b1b1b",
          "css_variable": "capylock-lock-bg",
          "tabs": []
        },
        {
          "name": "Gradient Color 1",
          "type": "color-picker",
          "on": "Gradient",
          "default": "#34c58e",
          "css_variable": "capylock-lock-bg-grad1",
          "tabs": []
        },
        {
          "name": "Gradient Color 2",
          "type": "color-picker",
          "on": "Gradient",
          "default": "#2e5f80",
          "css_variable": "capylock-lock-bg-grad2",
          "tabs": []
        },
        {
          "name": "Image Picker",
          "type": "image-picker",
          "on": "Image",
          "default": "",
          "css_variable": "capylock-lock-bg-img",
          "tabs": []
        }
      ]
    },
    "Layout": {
      "type": "dropdown",
      "default": "Horizontal",
      "values": {
        "Horizontal": {},
