{
    "cava": {
        "general": {
            "autosens": 1,
            "bars": 22,
            "framerate": 60,
            "higher_cutoff_freq": 12000,
            "lower_cutoff_freq": 50,
            "sensitivity": 100
        },
        "output": {
            "ascii_max_range": 100,
            "bit_format": "8bit",
            "channels": "mono",
            "data_format": "ascii",
            "method": "raw",
            "mono_option": "average"
        },
        "smoothing": {
            "monstercat": 0,
            "noise_reduction": 40
        }
    },
    "colors": {
        "currentTheme": "defaultTheme",
        "themes": {
            "defaultTheme": {
                "accent": "#86cecb",
                "base": "#FC20272f",
                "baseSolid": "#20272f",
                "dark": "#f20c0f12",
                "highlight": "#2b3b4a",
                "highlightAccent": "#a1f9f5",
                "highlightOverlay": "#10e7edf2",
                "inactive": "#a0d9dee2",
                "outline": "#d9dee2",
                "shadow": "#232230",
                "text": "#e7edf2",
                "urgent": "#e087b4"
            }
        }
    },
    "notifications": {
        "defaults": {
            "app": "",
            "mainIcon": "app",
            "showAppHint": true,
            "timeout": 5000
        },
        "overrides": [
        ]
    },
    "primaryScreen": "{{ .hostSettings.primaryDisplay }}",
    "style": {
        "bar": {
            "fontSize": 16,
            "height": 50,
            "opacity": 0.9,
            "radius": 20
        },
        "fonts": {
            "mono": "Monofur Nerd Font Mono",
            "pointSizeBase": 14,
            "propo": "Monofur Nerd Font Propo",
            "symbols": "Symbols Nerd Font",
            "symbolsMono": "Symbols Nerd Font Mono"
        },
        "rounding": {
            "large": 20,
            "medium": 14,
            "scale": 1,
            "small": 10,
            "xl": 28,
            "xs": 8
        },
        "spacing": {
            "lg": 32,
            "md": 20,
            "scale": 1,
            "sm": 14,
            "xl": 46,
            "xs": 8
        }
    }
}
